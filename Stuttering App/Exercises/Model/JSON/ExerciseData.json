              {
                 "id":"ex_3_2_pull_out",
                 "title":"Pull-Out",
                 "desc":"To relax muscles to slide out of the stutter.",
                 "category":"In-Block Correction",
                 "difficulty":"Advanced",
                 "word_start":1,
                 "meta":{
                    "duration_minutes":5,
                    "recommended_reps":5
                 },
                 "purpose":"To reduce tension and release a word while you are currently stuck.",
                 "instruction_set":{
                    "steps":[
                       {
                          "step_number":1,
                          "label":"Freeze",
                          "image":"",
                          "text":"Don't force it. Freeze exactly where you would stutter.",
                          "time":10
                       },
                       {
                          "step_number":2,
                          "label":"Locate Tension",
                          "image":"",
                          "text":"Where is the tightness? (Throat? Lips? Tongue?). Focus on it.",
                          "time":10
                       },
                       {
                          "step_number":3,
                          "label":"Melt & Slide",
                          "image":"",
                          "text":"Consciously relax that specific muscle while prolonging the sound, then slide out.",
                          "time":10
                       }
                    ],
                    "visual_cue":"",
                    "audio_guide_url":"path/to/pull_out_guide.mp3"
                 },
                 "example_demonstration":[
                 {
                    "target_sound":"He scored the 'Goal'.",
                    "execution_text":"'G-G-(Freeze)-(Relax Throat)-Ggggggoal.'"
                 }
                 ],

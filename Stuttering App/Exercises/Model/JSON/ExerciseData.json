              {
                 "id":"ex_3_3_Block_Correction",
                 "title":"Block Correction",
                 "desc":"To reset speech, then re-program the brain with fluency.",
                 "category":"Post-Block Correction",
                 "difficulty":"Advanced",
                 "word_start":1,
                 "meta":{
                    "duration_minutes":5,
                    "recommended_reps":5
                 },
                 "purpose":"To neutralize panic after a stutter and re-program the brain with a fluent correction.",
                 "instruction_set":{
                    "steps":[
                       {
                          "step_number":1,
                          "label":"Stop",
                          "image":"",
                          "text":"Finish the word/sentence, then stop completely for 3 seconds.",
                          "time":10
                       },
                       {
                          "step_number":2,
                          "label":"Reset",
                          "image":"",
                          "text":"Use this pause to neutralize the panic and calm the motor system.",
                          "time":5
                       },
                       {
                          "step_number":3,
                          "label":"Re-do",
                          "image":"",
                          "text":"Say the word again, but this time use a 'Light Contact' or 'Easy Onset'.",
                          "time":6
                       }
                    ],
                    "visual_cue":"",
                    "audio_guide_url":"path/to/cancellation_guide.mp3"
                 },
                 "example_demonstration":[
                 {
                    "target_sound":"I need to buy a 'Ticket'.",
                    "execution_text":"(Pause 3 seconds). (Internal thought: Light contact on C). 'Coffee.'"
                 }

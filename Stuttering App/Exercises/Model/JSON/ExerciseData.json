           "module_name":"Toolkit",
           "description":"These exercises are for when the user is ALREADY stuck or about to be stuck.",
           "module_id":"level_3_modification",
           "exercises":[
              {
                 "id":"ex_3_1_preparatory_set",
                 "title":"Preparatory Set",
                 "desc":"Practise a soft, easy start for clearer, effortless speaking.",
                 "category":"Anticipatory Management",
                 "difficulty":"Advanced",
                 "word_start":1,
                 "meta":{
                    "duration_minutes":5,
                    "recommended_reps":5
                 },
                 "purpose":"To plan a fluent onset when you anticipate a stutter on a specific word.",
                 "instruction_set":{
                    "steps":[
                       {
                          "step_number":1,
                          "label":"Pause",
                          "image":"",
                          "text":"Identify the word. Do not rush into the word. Stop immediately when you fear the word.",
                          "time":5
                       },
                       {
                          "step_number":2,
                          "label":"Map",
                          "image":"",
                          "text":"Mentally plan to use an 'Easy Onset' (for vowels) or 'Light Contact' (for consonants).",
                          "time":7
                       },
                       {
                          "step_number":3,
                          "label":"Action",
                          "image":"",
                          "text":"Say the word using the tool you planned.",
                          "time":5
                       }
                    ],
                    "visual_cue":"",
                    "audio_guide_url":"path/to/preparatory_set_guide.mp3"
                 },
                 "example_demonstration":[
                 {
                    "target_sound":"Can I ask you a 'Question'?",
                    "execution_text":"Can I ask you a 'Question'?"
                 }
                 ],
                 "data_bank":{
                    "description":"High-pressure words commonly associated with anticipation anxiety.",
                    "targets":{
                        "Examples":[
                            "Can I ask you a 'Question'?",

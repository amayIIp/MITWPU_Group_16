              {
                 "id":"ex_2_2_light_contacts",
                 "title":"Light Contacts",
                 "desc":"Practise a light touch to stop blocking air with lips.",
                 "category":"Articulation",
                 "target_group":"Plosives (P, B, T, D, K, G)",
                 "difficulty":"Intermediate",
                 "word_start":2,
                 "meta":{
                    "duration_minutes":5,
                    "recommended_reps":10
                 },
                 "purpose":"To prevent lips or tongue from pressing too tightly and blocking air.",
                 "instruction_set":{
                    "steps":[
                       {
                          "step_number":1,
                          "label":"Touch",
                          "image":"",
                          "text":"Bring your articulators (lips or tongue) together so lightly that they barely touch.",
                          "time":8
                       },
                       {
                          "step_number":2,
                          "label":"Initiation",
                          "image":"",
                          "text":"Initiate pronouncing the word from first letter making the sound 'mushy.'",
                          "time":6
                       },
                       {
                          "step_number":3,
                          "label":"Finish",
                          "image":"",
                          "text":"Don't let the air stop completely and finish the word.",
                          "time":5
                       }
                    ],
                    "visual_cue":"",
                    "audio_guide_url":"path/to/light_contact_guide.mp3"
                 },
                 "example_demonstration":[
                    {
                       "target_sound":"Ba-by",
                       "execution_text":"Barely touch lips. Sound almost like a 'V' or 'W'. 'Bvbaby'."
                    },
                    {
                       "target_sound":"Ti-me",
                       "execution_text":"Touch tongue tip feather-lightly to the roof of the mouth. 'Tthime'."
                    },
                    {
                       "target_sound":"Ki-te",
                       "execution_text":"Back of tongue touches roof of mouth gently. Let air leak through. 'Khh-ite'."
                    }
                 ],
